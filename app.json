
{
  "expo": {
    "name": "Lexo - Stock Management",
    "slug": "lexo-stock-management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/54f7086c-261a-4bd1-854e-d2de42aaed29.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/54f7086c-261a-4bd1-854e-d2de42aaed29.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5f5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lexo.stockmanagement",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/54f7086c-261a-4bd1-854e-d2de42aaed29.png",
        "backgroundColor": "#f5f5f5"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lexo.stockmanagement"
    },
    "web": {
      "favicon": "./assets/images/54f7086c-261a-4bd1-854e-d2de42aaed29.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "lexo",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
